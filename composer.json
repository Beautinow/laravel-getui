{
    "name": "shaozeming/laravel-getui",
    "description": "一个适用于laravel和lumen框架的个推push推送服务包",
    "type": "library",
    "require": {
        "laravel/framework": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "ShaoZeMing\\GeTui\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShaoZeMing\\GeTui\\Tests\\": "./tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shao ZeMing",
            "email": "szm19920426@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ShaoZeMing\\GeTui\\GeTuiServiceProvider"
            ],
            "alias": {
                "GeTui": "ShaoZeMing\\GeTui\\Facade\\GeTui"
            }
        }
    },
    "minimum-stability":"dev"
}
